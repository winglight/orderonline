										<div id="china" style="display: block;">
											<h1 class="content_box_left_main_left_h1"> 菜单</h1>
											<span class="content_box_left_main_left_c"> <!-- weij 2011-12-22 update -->
												
													<g:TextBox ui:field="queryKey" title="输入菜品名称" width="140px" /> <g:Anchor ui:field="querybtn" text="搜索" href="#"  />
												
												<!-- update end -->
												<h2 class="content_box_left_main_left_c_h2 bmargin cursor">
													<g:InlineLabel ui:field="menutypeAll" text="全部" />
												</h2>
												
												<g:FlowPanel ui:field="tabpage" />
												

											</span>
											<span class="content_box_left_main_left_b_2"></span>
										</div>
									</div>
									</div>
								</div>
								<div style="" id="c_content" class="content_box_left_main_right">
								          <h1 class="content_box_left_main_right_h1" id="tab_obj" style=""> <span>点击图片看产品详情</span> <a href="#" >看首页广告信息 &gt;&gt;</a> </h1>
									<!-- 产品信息 -->
									<div id="showProductsNew">
										<div id="showP_c" style="">
											<table width="100%" cellspacing="0" cellpadding="0"
												border="0">
												<tbody>
													<tr>
														<td class="content_box_left_main_right_c">
															<ul class="content_box_left_main_right_c_ul">

																<g:FlowPanel ui:field="resultPanel" />
															</ul>
															<div id="clear_div"></div>

															<div id="rewardlist_page2" class="{new1.pager}">
																<div class="{new1.pager-left}" style='display:none;'>
																	每页显示条数
																	<g:ListBox ui:field="pageNumber" styleName="{new2.select}" />
																</div>

																<div class="{new1.pager-right}">
																	<g:FlowPanel ui:field="resultpage" />
																</div>
															</div>
														</td>
													</tr>
												</tbody>
											</table>
											<span class="content_box_left_main_right_b"></span>
										</div>
									</div>
								</div>
							</div>
							<div id="clear_div"></div>
						</div>
					</div>
					    <div class="content_box_right_1" style="float: right"> <a href="#"><g:Image ui:field="ordernow" url="http://www.4008823823.com.cn/kfcios/images2/b5.gif" height="99" width="194" pixelSize="194,99" /> </a>
					      <div id="c"> <g:Image url="http://www.4008823823.com.cn/kfcios/images2/b5_3.gif" /><!-- weij 2012-01-10 更改路径 --> 
					      </div>
					      <div id="e" style="display: none;"> <g:Image url="http://www.4008823823.com.cn/kfcios/images2/b5_2.gif"  /> </div>
					    </div>
					<div id="orderArea" class="content_box_right" style="margin-top: 0px;">

										
						<div id="showOrder" class="content_box_right_dd">

							<div id="showO_c" style="">
