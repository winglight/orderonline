<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style src="new2.css" field="new2"></ui:style>
	<g:HTMLPanel>
		<div class="{new2.frame-wrap}">
			<!--面包屑 [[ -->
			<g:FlowPanel ui:field="breadCrumbs" />
			<!--面包屑 ]] -->


			<h3><g:InlineLabel ui:field="titleText" text="添加菜单类别" />
			</h3>
			<span class="{new2.cutline}"></span>

			<div class="{new2.content-wrap}">
				<div class="{new2.form}">
					<div class="{new2.form-line}">
						<label class="{new2.label}">RID：</label>
						<div class="{new2.element}">
							<g:InlineLabel ui:field="rid"  />
						</div>
					</div>
					<div class="{new2.form-line}">
						<label class="{new2.label2}"><span style="color:red">*</span>类名：</label>
						<div class="{new2.element}">
							<g:TextBox ui:field="name" styleName="{new2.text}" />
						</div>
					</div>
										<div class="{new2.form-line}">
						<label class="{new2.label2}"><span style="color:red">*</span>大类：</label>
						<div class="{new2.element}">
							<g:TextBox ui:field="dishestype" styleName="{new2.textleft}" />
							<g:FlowPanel ui:field="dishestypePanel" />
						</div>
					</div>

					<span class="{new2.cutline}"></span>
					<div class="{new2.form-line} {new2.form-line-btn}">
						<div class="{new2.element}">
							<span class="{new2.btn}">
								<g:Button ui:field="addBtn" text="保存" />
							</span>
						</div>
					</div>
				</div>
			</div>

		</div>

	</g:HTMLPanel>
</ui:UiBinder> 