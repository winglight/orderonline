<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style src="xxx.css" field="new2"></ui:style>
	<ui:style src="new2.css" field="new1"></ui:style>
	<g:HTMLPanel>


			<div class="public_container">
			
			

  
				<!--content_box start -->
				<div class="spx5px"></div>
				<div id="content" class="content_box">
					<!--content_box_left start -->
					<div class="content_box_left">
						<div style="margin-top:0px !important" class="content_box_left_main">
							<!-- 左侧菜单 -->
							<div id="showMenu" class="content_box_left_main_left">
								<div id="menu_china"
									style="width: 196px; overflow: hidden; float: left; background: none repeat scroll 0% 0% rgb(255, 255, 255); margin-top: 0px;">
